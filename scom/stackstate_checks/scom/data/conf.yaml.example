init_config:
    # runs every 5 minute
    min_collection_interval: 300

instances:
  - hostip: # SCOM IP address
    domain: # SCOM active directory domain
    username: # SCOM read-only username
    password: # SCOM password
    auth_mode: Network
    integration_mode: api # api or powershell
    max_number_of_requests: 10000
    criteria : "(FullName LIKE 'Microsoft.Windows.Computer:%')"
